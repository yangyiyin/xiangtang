CREATE TABLE `shopy_financial_department` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `type` int(10) DEFAULT '1' COMMENT '单位类型 如互助社,担保公司,银行',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '全称',
  `address` VARCHAR(60) DEFAULT ''  COMMENT '地址',
  `principal` VARCHAR(20) DEFAULT '' COMMENT '主要负责人',
	`phone` VARCHAR(50) DEFAULT '' COMMENT '电话',
	`fixed_phone` VARCHAR(50) DEFAULT '' COMMENT '固定电话',
	`tel_phone` VARCHAR(50) DEFAULT '' COMMENT '手机',
  `filler_man` VARCHAR(20) DEFAULT '' COMMENT '报表联系人',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

alter table shopy_financial_department add capital FLOAT(9,2) DEFAULT '0' COMMENT '注册资本 万元';
alter table shopy_financial_department add total_assets FLOAT(9,2) DEFAULT '0' COMMENT '总资产 万元';
alter table shopy_financial_department add staffs FLOAT(7,2) DEFAULT '0' COMMENT '员工人数';
alter table shopy_financial_department add capital_submit FLOAT(9,2) DEFAULT '0' COMMENT '实交注册资本 万元';
alter table shopy_financial_department add equity FLOAT(9,2) DEFAULT '0' COMMENT '所有者权益 万元';
alter table shopy_financial_department add build_time INT (11) DEFAULT '0' ;
alter table shopy_financial_department add sub_type INT (5) DEFAULT '0' ;

#财产保险公司月填报
CREATE TABLE `shopy_financial_insurance_property` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',
	`income` FLOAT(9,2) DEFAULT '0' COMMENT '保费收入 万元',
	`income_yoy` FLOAT(6,2) DEFAULT '0' COMMENT '保费同比增长',
	`income_a` FLOAT(9,2) DEFAULT '0' COMMENT '企业财产险 万元',
 	`income_b` FLOAT(9,2) DEFAULT '0' COMMENT '机动车险 万元',
 	`income_c` FLOAT(9,2) DEFAULT '0' COMMENT '其他险 万元',
 	`reserves` FLOAT(12,2) DEFAULT '0' COMMENT '储备金 万元',
  `payoff` FLOAT(9,2) DEFAULT '0' COMMENT '赔付金额 万元',
  `payoff_rate` FLOAT(6,2) DEFAULT '0' COMMENT '赔付率',
  `payoff_a` FLOAT(9,2) DEFAULT '0' COMMENT '企业财产险 万元',
  `payoff_a_rate` FLOAT(6,2) DEFAULT '0' COMMENT '企业财产险赔付率',
  `payoff_b` FLOAT(9,2) DEFAULT '0' COMMENT '机动车险 万元',
  `payoff_b_rate` FLOAT(6,2) DEFAULT '0' COMMENT '机动车险赔付率',
  `payoff_c` FLOAT(9,2) DEFAULT '0' COMMENT '其他险 万元',
  `payoff_c_rate` FLOAT(6,2) DEFAULT '0' COMMENT '其他险赔付率',
  `staff` int(6) DEFAULT '0' COMMENT '在岗人数',
  `authorized` int(6) DEFAULT '0' COMMENT '持证人数',
  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#人身保险公司月填报
CREATE TABLE `shopy_financial_insurance_life` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

	`income` FLOAT(9,2) DEFAULT '0' COMMENT '保费收入 万元',
	`income_a` FLOAT(6,2) DEFAULT '0' COMMENT '个人营销 万元',
	`income_a_yoy` FLOAT(9,2) DEFAULT '0' COMMENT '个人营销同比',
 	`income_a_a` FLOAT(9,2) DEFAULT '0' COMMENT '新单首期 万元',
 	`income_a_b` FLOAT(9,2) DEFAULT '0' COMMENT '新单期缴 万元',
 	`income_b` FLOAT(9,2) DEFAULT '0' COMMENT '团体业务 万元',
 	`income_c` FLOAT(9,2) DEFAULT '0' COMMENT '银行代理 万元',
  `payoff_a` FLOAT(9,2) DEFAULT '0' COMMENT '短险赔款金额 万元',
  `payoff_a_rate` FLOAT(6,2) DEFAULT '0' COMMENT '短险赔付率%',
  `payoff_b` FLOAT(9,2) DEFAULT '0' COMMENT '死伤医给付金额 万元',
  `payoff_c` FLOAT(9,2) DEFAULT '0' COMMENT '满期给付金额 万元',
   `payoff_d` FLOAT(9,2) DEFAULT '0' COMMENT '年金给付金额 万元',
   `backoff` FLOAT(9,2) DEFAULT '0' COMMENT '退保金额 万元',

  `staff` int(6) DEFAULT '0' COMMENT '在岗人数',
  `authorized` int(6) DEFAULT '0' COMMENT '持证人数',
  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;


#保险互助社月填报
CREATE TABLE `shopy_financial_insurance_mutual` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',


  Life_A int(11) NOT NULL DEFAULT 0,		#家财：承保件数
	Life_B int NOT NULL DEFAULT 0,		#承保户数
	Life_C FLOAT(7,2) NOT NULL DEFAULT 0,	#保费
	Life_D FLOAT(7,2) NOT NULL DEFAULT 0,	#保险金额(万元）
	Life_E int NOT NULL DEFAULT 0,		#赔付件数
	Life_F FLOAT(7,2) NOT NULL DEFAULT 0,	#赔付金额
	Casualty_A int NOT NULL DEFAULT 0,		#意外：承保件数
	Casualty_B int NOT NULL DEFAULT 0,		#承保户数
	Casualty_C FLOAT(7,2) NOT NULL DEFAULT 0,	#保费
	Casualty_D FLOAT(7,2) NOT NULL DEFAULT 0,	#保险金额(万元）
	Casualty_E int NOT NULL DEFAULT 0,		#赔付件数
	Casualty_F FLOAT(7,2) NOT NULL DEFAULT 0,	#赔付金额
	Medical_A int NOT NULL DEFAULT 0,		#医疗：承保件数
	Medical_B int NOT NULL DEFAULT 0,		#承保户数
	Medical_C FLOAT(7,2) NOT NULL DEFAULT 0,	#保费
	Medical_D FLOAT(7,2) NOT NULL DEFAULT 0,	#保险金额(万元）
	Medical_E int NOT NULL DEFAULT 0,		#赔付件数
	Medical_F FLOAT(7,2) NOT NULL DEFAULT 0,	#赔付金额
   
   

  `staff` int(6) DEFAULT '0' COMMENT '在岗人数',
  `authorized` int(6) DEFAULT '0' COMMENT '持证人数',
  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;


