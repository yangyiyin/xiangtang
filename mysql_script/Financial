#担保公司月填报
CREATE TABLE `shopy_financial_Vouch` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',
  `type` TINYINT(3) DEFAULT 1,#1年度 2历史累计

  C_Balance float(9,2) NOT NULL DEFAULT 0,	#本期余额
	C_Quantity int NOT NULL DEFAULT 0,		#本期笔数
	C_Balance_Ly float(9,2) NOT NULL DEFAULT 0,	#去年同期余额
	C_Quantity_Ly int NOT NULL DEFAULT 0,		#去年同期笔数
	G_Balance float(9,2) NOT NULL DEFAULT 0,	#本年余额
	G_Quantity int NOT NULL DEFAULT 0,		#本年笔数
	G_Balance_Ly float(9,2) NOT NULL DEFAULT 0,	#去年同期余额
	G_Quantity_Ly int NOT NULL DEFAULT 0,		#去年同期笔数
	C_Income float(7,2) NOT NULL DEFAULT 0,	#本月收入
	G_Income float(7,2) NOT NULL DEFAULT 0,	#本年收入
	C_Recover float(9,2) NOT NULL DEFAULT 0,	#本月收回
	G_Recover float(9,2) NOT NULL DEFAULT 0,	#本年收回
	C_Profit float(7,2) NOT NULL DEFAULT 0,	#本期利润
	G_Profit float(7,2) NOT NULL DEFAULT 0,	#本年利润
	C_Taxable float(7,2) NOT NULL DEFAULT 0,	#本期纳税总额
	G_Taxable float(7,2) NOT NULL DEFAULT 0,	#本年纳税总额

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

alter table shopy_financial_Vouch add C_Vouch FLOAT(7,2) DEFAULT '0'; #本期担保额

alter table shopy_financial_Vouch add G_Vouch FLOAT(7,2) DEFAULT '0'; #本年担保额
alter table shopy_financial_Vouch add G_Vouch_Ly FLOAT(7,2) DEFAULT '0'; #本年同期担保额




#股权投资/管理机构月填报
CREATE TABLE `shopy_financial_investment` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Types TINYINT(3) NOT NULL DEFAULT 0,			#类型 0:投资机构 1:管理机构
	Projects int NOT NULL DEFAULT 0,		#已投资项目总数/管理的机构数
	Projects_su int NOT NULL DEFAULT 0,		#属于初创期投资的项目数
	Amount float(8,2) NOT NULL DEFAULT 0,		#所投资项目总额/管理资金规模
	Amount_su float(8,2) NOT NULL DEFAULT 0,	#属于初创期投资的项目投资额
	Tax_B float(7,2) NOT NULL DEFAULT 0,		#营业税
	Tax_I float(7,2) NOT NULL DEFAULT 0,		#所得税
	Tax_O float(7,2) NOT NULL DEFAULT 0,		#其他税
	Staff int NOT NULL DEFAULT 0,			#从业人员数
	Staff_Sub nvarchar(100) NULL,			#男性,女性,大专,本科,硕士,博士,管理人员,劳务派遣,国内持证,国外持证

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

create table shopy_financial_investment_details (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

	Types TINYINT(3) NOT NULL DEFAULT 0,			#类型 0:投资机构 1:管理机构
	Name varchar(100) NULL,			#公司（项目）名称
	Area int NOT NULL DEFAULT 0,			#所在辖区分类
	Amount decimal(8,2) NOT NULL DEFAULT 0,		#投资/管理金额
	Remarks nvarchar(200) NULL,			#备注

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

create table shopy_financial_investment_exit (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Name varchar(100) NULL,			#项目名称
	Startdate int NOT NULL DEFAULT 0,			#投资起始日期
	Exitdate int NOT NULL DEFAULT 0,			#退出日期
	Area int NOT NULL DEFAULT 0,			#所在辖区分类
	Investment float(8,2) NOT NULL DEFAULT 0,	#投资额
	Recycling float(8,2) NOT NULL DEFAULT 0,	#回收额
	ExitMethod tinyint(3) NOT NULL DEFAULT 0,		#退出方式 1:上市退出 2:管理层回购退出 3:股权转让退出 4:企业并购退出 5:其他退出方式
	Remarks varchar(200) NULL,			#备注

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#期货
create table shopy_financial_futures (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  C_Volume int NOT NULL DEFAULT 0,			#成交量（万手）
  G_Volume int NOT NULL DEFAULT 0,			#本年成交量（万手）
	G_Volume_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%
	C_Turnover decimal(7,2) NOT NULL DEFAULT 0,	#成交额亿
	G_Turnover decimal(7,2) NOT NULL DEFAULT 0,	#成交额亿
	G_Turnover_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%
	C_Account int NOT NULL DEFAULT 0,			#新开户数
	G_Account int NOT NULL DEFAULT 0,			#新开户数
	G_Account_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%
	C_Assets decimal(7,2) NOT NULL DEFAULT 0,		#资产总值亿
	C_Assets_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%
	C_Profit decimal(7,2) NOT NULL DEFAULT 0,		#利润万
	C_Profit_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#融资租赁
create table shopy_financial_lease (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

	Capital decimal(8,2) NOT NULL DEFAULT 0,	#注册资本
	Owner decimal(8,2) NOT NULL DEFAULT 0,		#所有者权益
	Assets_M decimal(8,2) NOT NULL DEFAULT 0,	#租赁资产额-月
	Assets_Y decimal(8,2) NOT NULL DEFAULT 0,	#租赁资产额-年
	Business_Stay int NOT NULL DEFAULT 0,		#业务月末留存笔数
	Business_M_New int NOT NULL DEFAULT 0,		#当月新增笔数
	Business_Y_New int NOT NULL DEFAULT 0,		#本年新增笔数
	Business_T_New int NOT NULL DEFAULT 0,		#开业以来累计
	Client_Stay int NOT NULL DEFAULT 0,		#客户月末留存笔数
	Client_M_New int NOT NULL DEFAULT 0,		#当月新增笔数
	Client_Y_New int NOT NULL DEFAULT 0,		#本年新增笔数
	Client_T_New int NOT NULL DEFAULT 0,		#开业以来累计
	Business_C1 decimal(5,2) NOT NULL DEFAULT 0,	#经营-回租占比%
	Business_C2 decimal(5,2) NOT NULL DEFAULT 0,	#经营-收益率%
	Business_C3 decimal(5,2) NOT NULL DEFAULT 0,	#经营-回收率%
	Profit_A decimal(7,2) NOT NULL DEFAULT 0,	#当月营业收入
	Profit_B decimal(7,2) NOT NULL DEFAULT 0,	#当月累计净利润
	Profit_C decimal(7,2) NOT NULL DEFAULT 0,	#当月应缴增值税
	Profit_D decimal(7,2) NOT NULL DEFAULT 0,	#当月应缴所得税
	Profit_AY decimal(7,2) NOT NULL DEFAULT 0,	#本年累计营业收入
	Profit_BY decimal(7,2) NOT NULL DEFAULT 0,	#本年累计净利润
	Profit_CY decimal(7,2) NOT NULL DEFAULT 0,	#本年应缴增值税
	Profit_DY decimal(7,2) NOT NULL DEFAULT 0,	#本年应缴所得税

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

