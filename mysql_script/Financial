  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

create table shopy_financial_bank_baddebt_dispose (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Contract varchar(50) NULL,			#合同编号
	Enterprise varchar(200) NOT NULL,		#企业名称
	Loans decimal(9,2) NOT NULL DEFAULT 0,		#贷款余额（万元）
	Principal varchar(50) NULL,			#负责人
	Area int NOT NULL DEFAULT 0,			#所在辖区分类
	Recover decimal(9,2) NOT NULL DEFAULT 0,	#收回金额+
	Recover_Cash decimal(9,2) NOT NULL DEFAULT 0,	#现金
	Recover_Ot_1 decimal(9,2) NOT NULL DEFAULT 0,	#上划
	Recover_Ot_2 decimal(9,2) NOT NULL DEFAULT 0,	#以资抵债
	Recover_Ot_3 decimal(9,2) NOT NULL DEFAULT 0,	#重组上调
	Recover_Ot_4 decimal(9,2) NOT NULL DEFAULT 0,	#资产证券化
	Recover_Ot_5 decimal(9,2) NOT NULL DEFAULT 0,	#转让
	Recover_Ot_6 decimal(9,2) NOT NULL DEFAULT 0,	#核销
	Recover_Ot_7 decimal(9,2) NOT NULL DEFAULT 0,	#其他
	Remarks varchar(200) NULL,			#备注

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

alter table shopy_financial_bank_baddebt_dispose add Recover_Method tinyint(3) DEFAULT '0' ;
alter table shopy_financial_bank_baddebt_dispose add Recover_Time int(11) DEFAULT '0' ;

create table shopy_financial_bank_area_St (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  type tinyint(3) NOT NULL DEFAULT 1,
  Area int NOT NULL DEFAULT 0,			#辖区分类
	Baddebt_Firms int NOT NULL DEFAULT 0,		#本月不良贷款企业数
	Baddebt_Balance decimal(9,2) NOT NULL DEFAULT 0,#本月不良贷款余额
	Baddebt_Firms_add int NOT NULL DEFAULT 0,		#本月新增不良贷款企业数
	Baddebt_Balance_add decimal(9,2) NOT NULL DEFAULT 0,	#本月新增不良贷款余额
	Recover decimal(9,2) NOT NULL DEFAULT 0,	#收回金额+
	Recover_Cash decimal(9,2) NOT NULL DEFAULT 0,	#现金
	Recover_Ot_1 decimal(9,2) NOT NULL DEFAULT 0,	#上划
	Recover_Ot_2 decimal(9,2) NOT NULL DEFAULT 0,	#以资抵债
	Recover_Ot_3 decimal(9,2) NOT NULL DEFAULT 0,	#重组上调
	Recover_Ot_4 decimal(9,2) NOT NULL DEFAULT 0,	#资产证券化
	Recover_Ot_5 decimal(9,2) NOT NULL DEFAULT 0,	#转让
	Recover_Ot_6 decimal(9,2) NOT NULL DEFAULT 0,	#核销
	Recover_Ot_7 decimal(9,2) NOT NULL DEFAULT 0,	#其他
	Recover_Firms int NOT NULL DEFAULT 0,		#处置企业数

	Overdue_Firms int NOT NULL DEFAULT 0,		#本月逾期企业数
	Overdue_Balance decimal(9,2) NOT NULL DEFAULT 0,#本月逾期贷款余额
	Overdue_Firms_add int NOT NULL DEFAULT 0,		#本月新增逾期企业数
	Overdue_Balance_add decimal(9,2) NOT NULL DEFAULT 0,	#本月新增逾期余额
	Resolve_Firms int NOT NULL DEFAULT 0,		#化解企业数
	Resolve_Amount decimal(9,2) NOT NULL DEFAULT 0,	#化解金额

	Loans decimal(9,2) NOT NULL DEFAULT 0,		#贷款总额
	Pattern_1 decimal(9,2) NOT NULL DEFAULT 0,	#五级分类（正常）
	Pattern_2 decimal(9,2) NOT NULL DEFAULT 0,	#五级分类（关注）
	Pattern_3 decimal(9,2) NOT NULL DEFAULT 0,	#五级分类（次级）
	Pattern_4 decimal(9,2) NOT NULL DEFAULT 0,	#五级分类（可疑）
	Pattern_5 decimal(9,2) NOT NULL DEFAULT 0,	#五级分类（损失）
	Baddebt_Month decimal(9,2) NOT NULL DEFAULT 0,	#本月后三类额
	Baddebt_Lastmon decimal(9,2) NOT NULL DEFAULT 0,#上月后三类额
	Baddebt_Initial decimal(9,2) NOT NULL DEFAULT 0,#年初后三类额
	Baddebt_Lastyear decimal(9,2) NOT NULL DEFAULT 0,#去年后三类额
	Baddebt_Rate decimal(5,2) NOT NULL DEFAULT 0,		#本月不良贷款率
	Baddebt_Rate_Lastmon decimal(9,2) NOT NULL DEFAULT 0,	#上月不良贷款率
	Baddebt_Rate_Initial decimal(9,2) NOT NULL DEFAULT 0,	#年初不良贷款率
	Baddebt_Rate_Lastyear decimal(9,2) NOT NULL DEFAULT 0,	#去年不良贷款率

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;


create table shopy_financial_bank_quarterly (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Deposits decimal(7,2) NOT NULL DEFAULT 0,	#存款余额
	Loans decimal(7,2) NOT NULL DEFAULT 0,		#贷款余额
	Loans_A decimal(7,2) NOT NULL DEFAULT 0,	#公司贷款
	Loans_A_Sub varchar(50)NULL,				#其中政府重点项目,中小微企业
	Loans_B decimal(7,2) NOT NULL DEFAULT 0,	#按期限分
	Loans_B_Sub varchar(50) NULL,				#短期,中长期
	Loans_C decimal(7,2) NOT NULL DEFAULT 0,	#按产业分
	Loans_C_Sub varchar(100) NULL,				#第一产业,第二产业,纺织,家电,制造,零配件,制药,第三产业
	Loans_D decimal(7,2) NOT NULL DEFAULT 0,	#按担保方式分
	Loans_D_Sub varchar(100) NULL,				#抵押,其中动产抵押,金贝壳,保证,信用
	Loans_E decimal(7,2) NOT NULL DEFAULT 0,	#个人贷款
	Loans_E_Sub varchar(50)NULL,				#个人消费,其中个人住房,个人经营
	Loans_F decimal(7,2) NOT NULL DEFAULT 0,	#房地产贷款
	Loans_F_Sub decimal(7,2) NOT NULL DEFAULT 0,		#其中房地产开发
	Loans_G decimal(7,2) NOT NULL DEFAULT 0,	#支持地方经济发展贷款
	Loans_G_Sub decimal(7,2) NOT NULL DEFAULT 0,		#其中支持新区政府
	Loans_H decimal(7,2) NOT NULL DEFAULT 0,	#贷款质量
	Loans_H_Sub decimal(7,2) NOT NULL DEFAULT 0,		#风险贷款余额
	Loans_Ratio decimal(5,2) NOT NULL DEFAULT 0,		#存贷比
	Assets_Total decimal(7,2) NOT NULL DEFAULT 0,	#资产总计
	Debt_Total decimal(7,2) NOT NULL DEFAULT 0,	#负债总计
	Owner_Total decimal(7,2) NOT NULL DEFAULT 0,	#所有者权益
	Profit_Total decimal(7,2) NOT NULL DEFAULT 0,	#累计利润
	Tax_Total decimal(7,2) NOT NULL DEFAULT 0,	#各类税费
	Other_Item decimal(7,2) NOT NULL DEFAULT 0,	#表外项目余额
	Other_Item_Sub varchar(50) NULL,			#承兑,票据,信用证,信托,其他
	Staff int NOT NULL DEFAULT 0,			#从业人员数
	Staff_Sub varchar(100) NULL,			#男性,女性,大专,本科,硕士,博士,管理人员,劳务派遣,国内持证,国外持证

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;



create table shopy_financial_enterprise (
 `id` int(11) NOT NULL AUTO_INCREMENT,

RegNum varchar(50)NULL,
CreditCode varchar(50) NULL,
Name varchar(50) NULL,
Legal varchar(20) NULL,
Founded varchar(20) NULL,
Jurisdictions varchar(50) NULL,
Type varchar(50) NULL,
Capital varchar(20) NULL,
Approval varchar(20) NULL,
Industry varchar(50) NULL,
Scope varchar(1024) NULL,
Address varchar(50) NULL,
Phone varchar(20) NULL,
IDcard varchar(30) NULL,
Contacts varchar(10) NULL,
Mobile varchar(20) NULL,
Circle varchar(10) NULL,
RemoveTag varchar(10) NULL,
SyncMark varchar(20) NULL,

  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;


