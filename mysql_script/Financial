 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
