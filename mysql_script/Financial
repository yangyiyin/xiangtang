	Casualty_A int NOT NULL DEFAULT 0,		#意外：承保件数
	Casualty_B int NOT NULL DEFAULT 0,		#承保户数
	Casualty_C FLOAT(7,2) NOT NULL DEFAULT 0,	#保费
	Casualty_D FLOAT(7,2) NOT NULL DEFAULT 0,	#保险金额(万元）
	Casualty_E int NOT NULL DEFAULT 0,		#赔付件数
	Casualty_F FLOAT(7,2) NOT NULL DEFAULT 0,	#赔付金额
	Medical_A int NOT NULL DEFAULT 0,		#医疗：承保件数
	Medical_B int NOT NULL DEFAULT 0,		#承保户数
	Medical_C FLOAT(7,2) NOT NULL DEFAULT 0,	#保费
	Medical_D FLOAT(7,2) NOT NULL DEFAULT 0,	#保险金额(万元）
	Medical_E int NOT NULL DEFAULT 0,		#赔付件数
	Medical_F FLOAT(7,2) NOT NULL DEFAULT 0,	#赔付金额

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#担保公司月填报
CREATE TABLE `shopy_financial_Vouch` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',
  `type` TINYINT(3) DEFAULT 1,#1年度 2历史累计

  C_Balance float(9,2) NOT NULL DEFAULT 0,	#本期余额
	C_Quantity int NOT NULL DEFAULT 0,		#本期笔数
	C_Balance_Ly float(9,2) NOT NULL DEFAULT 0,	#去年同期余额
	C_Quantity_Ly int NOT NULL DEFAULT 0,		#去年同期笔数
	G_Balance float(9,2) NOT NULL DEFAULT 0,	#本年余额
	G_Quantity int NOT NULL DEFAULT 0,		#本年笔数
	G_Balance_Ly float(9,2) NOT NULL DEFAULT 0,	#去年同期余额
	G_Quantity_Ly int NOT NULL DEFAULT 0,		#去年同期笔数
	C_Income float(7,2) NOT NULL DEFAULT 0,	#本月收入
	G_Income float(7,2) NOT NULL DEFAULT 0,	#本年收入
	C_Recover float(9,2) NOT NULL DEFAULT 0,	#本月收回
	G_Recover float(9,2) NOT NULL DEFAULT 0,	#本年收回
	C_Profit float(7,2) NOT NULL DEFAULT 0,	#本期利润
	G_Profit float(7,2) NOT NULL DEFAULT 0,	#本年利润
	C_Taxable float(7,2) NOT NULL DEFAULT 0,	#本期纳税总额
	G_Taxable float(7,2) NOT NULL DEFAULT 0,	#本年纳税总额

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

alter table shopy_financial_Vouch add C_Vouch FLOAT(7,2) DEFAULT '0'; #本期担保额

alter table shopy_financial_Vouch add G_Vouch FLOAT(7,2) DEFAULT '0'; #本年担保额
alter table shopy_financial_Vouch add G_Vouch_Ly FLOAT(7,2) DEFAULT '0'; #本年同期担保额




#股权投资/管理机构月填报
CREATE TABLE `shopy_financial_investment` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Types TINYINT(3) NOT NULL DEFAULT 0,			#类型 0:投资机构 1:管理机构
	Projects int NOT NULL DEFAULT 0,		#已投资项目总数/管理的机构数
	Projects_su int NOT NULL DEFAULT 0,		#属于初创期投资的项目数
	Amount float(8,2) NOT NULL DEFAULT 0,		#所投资项目总额/管理资金规模
	Amount_su float(8,2) NOT NULL DEFAULT 0,	#属于初创期投资的项目投资额
	Tax_B float(7,2) NOT NULL DEFAULT 0,		#营业税
	Tax_I float(7,2) NOT NULL DEFAULT 0,		#所得税
	Tax_O float(7,2) NOT NULL DEFAULT 0,		#其他税
	Staff int NOT NULL DEFAULT 0,			#从业人员数
	Staff_Sub nvarchar(100) NULL,			#男性,女性,大专,本科,硕士,博士,管理人员,劳务派遣,国内持证,国外持证

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

create table shopy_financial_investment_details (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

	Types TINYINT(3) NOT NULL DEFAULT 0,			#类型 0:投资机构 1:管理机构
	Name varchar(100) NULL,			#公司（项目）名称
	Area int NOT NULL DEFAULT 0,			#所在辖区分类
	Amount decimal(8,2) NOT NULL DEFAULT 0,		#投资/管理金额
	Remarks nvarchar(200) NULL,			#备注

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

create table shopy_financial_investment_exit (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Name varchar(100) NULL,			#项目名称
	Startdate int NOT NULL DEFAULT 0,			#投资起始日期
	Exitdate int NOT NULL DEFAULT 0,			#退出日期
	Area int NOT NULL DEFAULT 0,			#所在辖区分类
	Investment float(8,2) NOT NULL DEFAULT 0,	#投资额
	Recycling float(8,2) NOT NULL DEFAULT 0,	#回收额
	ExitMethod tinyint(3) NOT NULL DEFAULT 0,		#退出方式 1:上市退出 2:管理层回购退出 3:股权转让退出 4:企业并购退出 5:其他退出方式
	Remarks varchar(200) NULL,			#备注

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#期货
create table shopy_financial_futures (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  C_Volume int NOT NULL DEFAULT 0,			#成交量（万手）
  G_Volume int NOT NULL DEFAULT 0,			#本年成交量（万手）
	G_Volume_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%
	C_Turnover decimal(7,2) NOT NULL DEFAULT 0,	#成交额亿
	G_Turnover decimal(7,2) NOT NULL DEFAULT 0,	#成交额亿
	G_Turnover_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%
	C_Account int NOT NULL DEFAULT 0,			#新开户数
	G_Account int NOT NULL DEFAULT 0,			#新开户数
	G_Account_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%
	C_Assets decimal(7,2) NOT NULL DEFAULT 0,		#资产总值亿
	C_Assets_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%
	C_Profit decimal(7,2) NOT NULL DEFAULT 0,		#利润万
	C_Profit_yoy decimal(5,2) NOT NULL DEFAULT 0,	#同比%

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#融资租赁
create table shopy_financial_lease (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

	Capital decimal(8,2) NOT NULL DEFAULT 0,	#注册资本
	Owner decimal(8,2) NOT NULL DEFAULT 0,		#所有者权益
	Assets_M decimal(8,2) NOT NULL DEFAULT 0,	#租赁资产额-月
	Assets_Y decimal(8,2) NOT NULL DEFAULT 0,	#租赁资产额-年
	Business_Stay int NOT NULL DEFAULT 0,		#业务月末留存笔数
	Business_M_New int NOT NULL DEFAULT 0,		#当月新增笔数
	Business_Y_New int NOT NULL DEFAULT 0,		#本年新增笔数
	Business_T_New int NOT NULL DEFAULT 0,		#开业以来累计
	Client_Stay int NOT NULL DEFAULT 0,		#客户月末留存笔数
	Client_M_New int NOT NULL DEFAULT 0,		#当月新增笔数
	Client_Y_New int NOT NULL DEFAULT 0,		#本年新增笔数
	Client_T_New int NOT NULL DEFAULT 0,		#开业以来累计
	Business_C1 decimal(5,2) NOT NULL DEFAULT 0,	#经营-回租占比%
	Business_C2 decimal(5,2) NOT NULL DEFAULT 0,	#经营-收益率%
	Business_C3 decimal(5,2) NOT NULL DEFAULT 0,	#经营-回收率%
	Profit_A decimal(7,2) NOT NULL DEFAULT 0,	#当月营业收入
	Profit_B decimal(7,2) NOT NULL DEFAULT 0,	#当月累计净利润
	Profit_C decimal(7,2) NOT NULL DEFAULT 0,	#当月应缴增值税
	Profit_D decimal(7,2) NOT NULL DEFAULT 0,	#当月应缴所得税
	Profit_AY decimal(7,2) NOT NULL DEFAULT 0,	#本年累计营业收入
	Profit_BY decimal(7,2) NOT NULL DEFAULT 0,	#本年累计净利润
	Profit_CY decimal(7,2) NOT NULL DEFAULT 0,	#本年应缴增值税
	Profit_DY decimal(7,2) NOT NULL DEFAULT 0,	#本年应缴所得税

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#小额贷款
create table shopy_financial_loan (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

	Funds_Owner decimal(9,2) NOT NULL DEFAULT 0,	#所者权益
	Funds_Bank decimal(9,2) NOT NULL DEFAULT 0,	#银行融资
	Month_Amount decimal(9,2) NOT NULL DEFAULT 0,	#月末贷款余额
	Month_Amount_N int NOT NULL DEFAULT 0,		#月末贷款笔数
	Month_Small decimal(9,2) NOT NULL DEFAULT 0,	#小额贷款
	Month_Small_N int NOT NULL DEFAULT 0,		#笔数
	Year_Amount decimal(9,2) NOT NULL DEFAULT 0,	#本年贷款金额
	Year_Amount_N int NOT NULL DEFAULT 0,		#本年贷款笔数
	Year_Small decimal(9,2) NOT NULL DEFAULT 0,	#小额贷款
	Year_Small_N int NOT NULL DEFAULT 0,		#笔数
	Total_Amount decimal(9,2) NOT NULL DEFAULT 0,	#开业以来贷款金额
	Total_Amount_N int NOT NULL DEFAULT 0,		#开业以来贷款笔数
	Total_Small decimal(9,2) NOT NULL DEFAULT 0,	#小额贷款
	Total_Small_N int NOT NULL DEFAULT 0,		#笔数
	Interest_Rate decimal(5,2) NOT NULL DEFAULT 0,	#年利率%
	Bad_Debt decimal(9,2) NOT NULL DEFAULT 0,	#不良贷款
	Net_Profit decimal(9,2) NOT NULL DEFAULT 0,	#净利润
	Revenue decimal(9,2) NOT NULL DEFAULT 0,	#总收入

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#证券公司
create table shopy_financial_securities (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

	Volume decimal(7,2) NOT NULL DEFAULT 0,		#成交量亿
	Volume_yoy decimal(6,2) NOT NULL DEFAULT 0,	#同比%
	G_Volume decimal(8,2) NOT NULL DEFAULT 0,		#成交量亿 本年累计
	G_Volume_yoy decimal(6,2) NOT NULL DEFAULT 0,	#同比% 本年累计
	New_Account int NOT NULL DEFAULT 0,		#新开户数
	New_Account_yoy decimal(6,2) NOT NULL DEFAULT 0,#同比%
	G_New_Account int NOT NULL DEFAULT 0,		#新开户数 本年累计
	G_New_Account_yoy decimal(6,2) NOT NULL DEFAULT 0,#同比%
	Assets decimal(7,2) NOT NULL DEFAULT 0,		#资产总值
	Assets_yoy decimal(6,2) NOT NULL DEFAULT 0,	#同比%
	Profit decimal(7,2) NOT NULL DEFAULT 0,		#利润万
	Profit_yoy decimal(6,2) NOT NULL DEFAULT 0,	#同比%

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;



#转贷明细
create table shopy_financial_transfer_funds (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

	Bank varchar(100) NULL,			#转贷银行
	Account varchar(50) NULL,			#帐号
	Unit varchar(100) NULL,			#转贷单位
	Legal_Person varchar(20) NULL,			#法人代表
	Amount decimal(7,2) NOT NULL DEFAULT 0,		#转贷金额
	S_Date int(11) NOT NULL,			#起始日期
	E_Date int(11) NOT NULL,			#到期日期
	Days int(11) NOT NULL DEFAULT 0,			#天数 datediff(E_Date,S_Date)
	Remarks varchar(200) NULL,			#备注

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#转贷统计
create table shopy_financial_transfer_funds_st (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',
  type tinyint(3)  DEFAULT 1,
	M_Amount decimal(7,2) NOT NULL DEFAULT 0,	#本月金额
	M_Quantity int NOT NULL DEFAULT 0,		#笔数
	Y_Amount decimal(7,2) NOT NULL DEFAULT 0,	#本年金额
	Y_Quantity int NOT NULL DEFAULT 0,		#笔数
	T_Amount decimal(7,2) NOT NULL DEFAULT 0,	#总计金额
	T_Quantity int NOT NULL DEFAULT 0,		#笔数

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;



#银行信贷
create table shopy_financial_bank_credit (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Deposits decimal(7,2) NOT NULL DEFAULT 0,	#存款余额	亿元
	Deposit_A decimal(7,2) NOT NULL DEFAULT 0,	#一、境内存款
	Deposit_A1 decimal(7,2) NOT NULL DEFAULT 0,	#1、住户存款
	Deposit_A11 decimal(7,2) NOT NULL DEFAULT 0,	#1.1、住户存款
	Deposit_A12 decimal(7,2) NOT NULL DEFAULT 0,	#1.2、活期存款
	Deposit_A2 decimal(7,2) NOT NULL DEFAULT 0,	#2、非金融企业存款
	Deposit_A21 decimal(7,2) NOT NULL DEFAULT 0,	#2.1、活期存款
	Deposit_A22 decimal(7,2) NOT NULL DEFAULT 0,	#2.2、定期及其他存款
	Deposit_A3 decimal(7,2) NOT NULL DEFAULT 0,	#3、广义政府存款
	Deposit_A31 decimal(7,2) NOT NULL DEFAULT 0,	#3.1、活期存款
	Deposit_A32 decimal(7,2) NOT NULL DEFAULT 0,	#3.2、定期及其他存款
	Deposit_A4 decimal(7,2) NOT NULL DEFAULT 0,	#4、非银行业金融机构存款
	Deposit_B decimal(7,2) NOT NULL DEFAULT 0,	#二、境外存款
	Deposits_Initial decimal(7,2) NOT NULL DEFAULT 0,	#年初余额
	Deposits_Lastmon decimal(7,2) NOT NULL DEFAULT 0,	#上月余额
	Deposits_Lastyear decimal(7,2) NOT NULL DEFAULT 0,	#去年同期
	Loans decimal(7,2) NOT NULL DEFAULT 0,		#贷款余额
	Loan_A decimal(7,2) NOT NULL DEFAULT 0,		#境内贷款
	Loan_A1 decimal(7,2) NOT NULL DEFAULT 0,	#1、住户贷款
	Loan_A11 decimal(7,2) NOT NULL DEFAULT 0,	#1.1、短期贷款
	Loan_A111 decimal(7,2) NOT NULL DEFAULT 0,	#1.1.1、消费贷款
	Loan_A112 decimal(7,2) NOT NULL DEFAULT 0,	#1.1.2、经营贷款
	Loan_A12 decimal(7,2) NOT NULL DEFAULT 0,	#1.2、中长期贷款
	Loan_A121 decimal(7,2) NOT NULL DEFAULT 0,	#1.2.1、消费贷款
	Loan_A122 decimal(7,2) NOT NULL DEFAULT 0,	#1.2.2、经营贷款
	Loan_A2 decimal(7,2) NOT NULL DEFAULT 0,	#2、非金融企业及机关团体贷款
	Loan_A21 decimal(7,2) NOT NULL DEFAULT 0,	#2.1、短期贷款
	Loan_A22 decimal(7,2) NOT NULL DEFAULT 0,	#2.2、中长期贷款
	Loan_A23 decimal(7,2) NOT NULL DEFAULT 0,	#2.3、票据融资
	Loan_A24 decimal(7,2) NOT NULL DEFAULT 0,	#2.4、融资租赁
	Loan_A25 decimal(7,2) NOT NULL DEFAULT 0,	#2.5、各项垫款
	Loan_A3 decimal(7,2) NOT NULL DEFAULT 0,	#3、非银行业金融机构贷款
	Loan_B decimal(7,2) NOT NULL DEFAULT 0,		#境外贷款
	Loan_Initial decimal(7,2) NOT NULL DEFAULT 0,	#年初余额
	Loan_Lastmon decimal(7,2) NOT NULL DEFAULT 0,	#上月余额
	Loan_Lastyear decimal(7,2) NOT NULL DEFAULT 0,	#去年同期

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

#银行信贷统计
create table shopy_financial_bank_credit_st (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Deposits decimal(7,2) NOT NULL DEFAULT 0,	#存款余额	亿元
	Deposit_A decimal(7,2) NOT NULL DEFAULT 0,	#一、境内存款
	Deposit_A1 decimal(7,2) NOT NULL DEFAULT 0,	#1、住户存款
	Deposit_A2 decimal(7,2) NOT NULL DEFAULT 0,	#2、非金融企业存款
	Deposit_A3 decimal(7,2) NOT NULL DEFAULT 0,	#3、广义政府存款
	Deposit_A4 decimal(7,2) NOT NULL DEFAULT 0,	#4、非银行业金融机构存款
	Deposit_B decimal(7,2) NOT NULL DEFAULT 0,	#二、境外存款
	Loans decimal(7,2) NOT NULL DEFAULT 0,		#贷款余额
	Loan_A decimal(7,2) NOT NULL DEFAULT 0,		#境内贷款
	Loan_A1 decimal(7,2) NOT NULL DEFAULT 0,	#1、住户贷款
	Loan_A2 decimal(7,2) NOT NULL DEFAULT 0,	#2、非金融企业及机关团体贷款
	Loan_A3 decimal(7,2) NOT NULL DEFAULT 0,	#3、非银行业金融机构贷款
	Loan_B decimal(7,2) NOT NULL DEFAULT 0,		#境外贷款

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

create table shopy_financial_bank_loan_detail (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Contract varchar(50) NULL,			#合同编号
	Enterprise varchar(200) NOT NULL,		#企业名称
	Loans decimal(9,2) NOT NULL DEFAULT 0,		#贷款余额（万元）
	Interest decimal(5,2) NOT NULL DEFAULT 0,	#年利率
	Principal varchar(50) NULL,			#负责人
	Phone varchar(50) NULL,			#联系电话
	Address varchar(100) NULL,			#注册地址
	Area int NOT NULL DEFAULT 0,			#所在辖区分类
	Industry varchar(100) NULL,			#所属行业
	Startdate int NOT NULL DEFAULT 0,			#发放日期
	Enddate int NOT NULL DEFAULT 0,			#到期日期
	Guarantee varchar(20) NULL,			#担保方式
	Over_Credit decimal(9,2) NOT NULL DEFAULT 0,	#信用余额
	Over_Mortgage decimal(9,2) NOT NULL DEFAULT 0,	#抵押余额
	Over_Pledge decimal(9,2) NOT NULL DEFAULT 0,	#质押余额
	Over_Margin decimal(9,2) NOT NULL DEFAULT 0,	#保证余额
	Guarantor varchar(200) NULL,			#保证人
	Pattern tinyint(3) NOT NULL DEFAULT 0,		#五级形态（填写：正常、关注、次级、可疑、损失）
	OverdueDays int NOT NULL DEFAULT 0,		#逾期天数
	Coordination tinyint(3) NOT NULL DEFAULT 0,		#是否需要政府配合
	Remarks varchar(200) NULL,			#备注

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;

create table shopy_financial_bank_baddebt_st (
 `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT '0' COMMENT '用户id',
  `all_name` VARCHAR(50) DEFAULT '' COMMENT '公司全称',
  `filler_man` VARCHAR(50) DEFAULT ''  COMMENT '填报人',
  `year` VARCHAR(5) DEFAULT '' COMMENT '年份',
  `month` VARCHAR(10) DEFAULT '' COMMENT '月份',

  Loans decimal(9,2) NOT NULL DEFAULT 0,		# 贷款总额
	Pattern_1 decimal(9,2) NOT NULL DEFAULT 0,	# 五级分类（正常）
	Pattern_2 decimal(9,2) NOT NULL DEFAULT 0,	# 五级分类（关注）
	Pattern_3 decimal(9,2) NOT NULL DEFAULT 0,	# 五级分类（次级）
	Pattern_4 decimal(9,2) NOT NULL DEFAULT 0,	# 五级分类（可疑）
	Pattern_5 decimal(9,2) NOT NULL DEFAULT 0,	# 五级分类（损失）
	Baddebt_Month decimal(9,2) NOT NULL DEFAULT 0,	# 本月后三类额
	Baddebt_Lastmon decimal(9,2) NOT NULL DEFAULT 0,# 上月后三类额
	Baddebt_Initial decimal(9,2) NOT NULL DEFAULT 0,#  年初后三类额
	Baddebt_Lastyear decimal(9,2) NOT NULL DEFAULT 0,# 去年后三类额
	Baddebt_Rate decimal(5,2) NOT NULL DEFAULT 0,		# 本月不良贷款率
	Baddebt_Rate_Lastmon decimal(9,2) NOT NULL DEFAULT 0,	#上月不良贷款率
	Baddebt_Rate_Initial decimal(9,2) NOT NULL DEFAULT 0,	#年初不良贷款率
	Baddebt_Rate_Lastyear decimal(9,2) NOT NULL DEFAULT 0,	#去年不良贷款率

  `ip` VARCHAR(50) DEFAULT '' COMMENT 'ip',
  `gmt_create` int(11) DEFAULT 0,
  `deleted` TINYINT(3) DEFAULT 0,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_bin;


